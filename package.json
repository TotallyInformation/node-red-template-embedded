{
  "name": "node-red-template-embedded",
  "version": "0.0.7",
  "description": "A template project for Node-Red in embedded mode allowing multiple instances",
  "private": true,
  "readme": "README.md",
  "keywords": [
    "node-red"
  ],
  "contributors": [
    {
      "name": "Julian Knight",
      "url": "https://it.knightnet.org.uk"
    },
    "sebastian.barwe@gmail.com"
  ],
  "repository": "TotallyInformation/node-red-template-embedded",
  "author": {
    "name": "Julian Knight (TotallyInformation)",
    "url": "https://www.totallyinformation.com"
  },
  "homepage": "https://github.com/TotallyInformation/node-red-template-embedded",
  "license": "MIT",
  "bugs": "https://github.com/TotallyInformation/node-red-template-embedded/issues",
  "main": "server.js",
  "config": {
    "http_port": "1880",
    "use_https": "false",
    "listening_address": "localhost",
    "nr_userfolder": "./.nodered",
    "nr_flowfile": "./flows.json",
    "nr_credentialsecret": null,
    "nr_title": "Node RED Embedded"
  },
  "scripts": {
    "start": "node src/server.js",
    "ssl": "npm start --node-red-template-embedded:http_port=1882 --node-red-template-embedded:use_https=true",
    "debug": "cross-env NODE_ENV=development \"nodemon --config .nodemonrc.json --verbose --watch ./src --watch ./node_modules --watch ./nodes --ignore *.log --ext js,html --debug src/server.js\"",
    "admin": "node-red-admin",
    "adminauth": "node -e \"console.log(require('bcryptjs').hashSync(process.argv[1], 8));\"",
    "selfsigned": "node -e \"var pems = require('selfsigned').generate([{ name: 'commonName', value: process.argv[1] }], { days: 365 }); var fs = require('fs'); fs.writeFileSync('server.key', pems.private); fs.writeFileSync('server.crt', pems.cert); console.log('Updated server.key|crt for CN', process.argv[1]);\""
  },
  "devDependencies": {
    "nodemon":"^1.11.0"
  },
  "optionalDependencies": {
    "cross-env": "4.*",
    "selfsigned": "1.8.*",
    "bcryptjs": "^2.4.0"
  },
  "dependencies": {
    "express": "4.*",
    "node-red": ">=0.17.3",
    "paint-console": "0.0.1",
    "syslog-client": "^1.1.0",
    "node-red-contrib-contextbrowser" :">=0.0.4"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
